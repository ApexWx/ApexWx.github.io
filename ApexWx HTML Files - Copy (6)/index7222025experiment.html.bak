    <script> // Seasons
  function getCurrentSeason(date = new Date()) {
    const month = date.getMonth() + 1;
    const day = date.getDate();

    if ((month === 12 && day >= 21) || (month <= 2) || (month === 3 && day < 20)) {
      return "winter";
    } else if ((month === 3 && day >= 20) || (month >= 4 && month <= 5) || (month === 6 && day < 21)) {
      return "spring";
    } else if ((month === 6 && day >= 21) || (month >= 7 && month <= 8) || (month === 9 && day < 22)) {
      return "summer";
    } else {
      return "fall";
    }
  }

  function getSeasonEmoji(season) {
    switch (season) {
      case "winter": return "🌨️";
      case "spring": return "🌸";
      case "summer": return "☀️";
      case "fall":   return "🍂";
      default: return "";
    }
  }

  document.addEventListener("DOMContentLoaded", function() {
    const season = getCurrentSeason();
    const emoji = getSeasonEmoji(season);
    const displayDiv = document.getElementById("season-display");

    if (displayDiv) {
      const capitalizedSeason = season.charAt(0).toUpperCase() + season.slice(1);
      displayDiv.textContent = `${emoji} Current season: ${capitalizedSeason}`;
      displayDiv.classList.add(season); // Adds color class
    }
  });
</script> <script> // Sun Times Fort Kent, Maine
  async function fetchSunriseSunset() {
    const lat = 47.2632;  // Fort Kent, ME
    const lng = -68.5774;
    const apiURL = `https://api.sunrise-sunset.org/json?lat=${lat}&lng=${lng}&formatted=0`;

    try {
      const response = await fetch(apiURL);
      const data = await response.json();

      const sunriseUTC = new Date(data.results.sunrise);
      const sunsetUTC = new Date(data.results.sunset);
      const dayLengthSec = data.results.day_length;

      // Convert UTC times to local (Eastern Time)
      const options = { hour: 'numeric', minute: '2-digit', timeZone: 'America/New_York' };
      const sunriseLocal = sunriseUTC.toLocaleTimeString('en-US', options);
      const sunsetLocal = sunsetUTC.toLocaleTimeString('en-US', options);

      // Get EDT/EST abbreviation safely
      const tzString = new Date().toLocaleTimeString('en-US', {
        timeZone: 'America/New_York',
        timeZoneName: 'short'
      });
      const tzAbbr = tzString.match(/\b(EDT|EST)\b/)?.[0] || "ET";

      // Format current date
      const now = new Date();
      const dateStr = now.toLocaleDateString('en-US', { weekday: 'long', year: 'numeric', month: 'long', day: 'numeric' });

      // Convert daylight length from seconds to hh:mm
      const hours = Math.floor(dayLengthSec / 3600);
      const minutes = Math.floor((dayLengthSec % 3600) / 60);
      const dayLengthStr = `${hours}h ${minutes}m`;

   document.getElementById("sunriseSunset").innerHTML = `
    <div class="sun-info">
    <div>🌅 <strong>Sunrise</strong>: ${sunriseLocal} ${tzAbbr} | </div>
    <div>🌇<strong>Sunset</strong>: ${sunsetLocal} ${tzAbbr} | </div>
    <div><strong>Day Length</strong>: ${dayLengthStr}</div>
  </div>
`;

    } catch (error) {
      console.error("Sunrise/Sunset fetch error:", error);
      document.getElementById("sunriseSunset").innerText = "Unable to load sunrise/sunset.";
    }
  }

  fetchSunriseSunset();
  setInterval(fetchSunriseSunset, 3600000); // Refresh every hour
</script> <script> // Local Time
  function updateLocalTime() {
    const now = new Date();

    const days = ["Sunday", "Monday", "Tuesday", "Wednesday", "Thursday", "Friday", "Saturday"];
    const months = [
      "January", "February", "March", "April", "May", "June",
      "July", "August", "September", "October", "November", "December"
    ];

    const dayName = days[now.getDay()];
    const monthName = months[now.getMonth()];
    const day = now.getDate();
    const year = now.getFullYear();

    let hour = now.getHours();
    const minute = now.getMinutes().toString().padStart(2, '0');
    const ampm = hour >= 12 ? 'pm' : 'am';
    hour = hour % 12 || 12;

    const options = { timeZoneName: 'short' };
    const tz = new Intl.DateTimeFormat('en-US', options).formatToParts(now)
      .find(part => part.type === 'timeZoneName').value;

    const output = `${dayName}, ${monthName} ${day}, ${year} at ${hour}:${minute} ${ampm} ${tz}`;

    document.getElementById("localTime").textContent = output;
  }

  updateLocalTime(); // Initial call
  setInterval(updateLocalTime, 60000); // Update every minute
</script> <script> // 24-Hour Clocks
  function updateDualTime() {
    const now = new Date();

    // Format local time (24-hour) with EST/EDT abbreviation
    const localHour = now.getHours().toString().padStart(2, '0');
    const localMinute = now.getMinutes().toString().padStart(2, '0');
    const tz = new Intl.DateTimeFormat('en-US', { timeZoneName: 'short' })
      .formatToParts(now)
      .find(part => part.type === 'timeZoneName').value;
    const localTime = `${localHour}:${localMinute} ${tz}`;

    // Format UTC time (24-hour)
    const utcHour = now.getUTCHours().toString().padStart(2, '0');
    const utcMinute = now.getUTCMinutes().toString().padStart(2, '0');
    const utcTime = `${utcHour}:${utcMinute} UTC`;

    // Output to div
    document.getElementById("dualTime").innerHTML = `${localTime}<br>${utcTime}`;
  }

  updateDualTime(); // Initial call
  setInterval(updateDualTime, 60000); // Update every minute
