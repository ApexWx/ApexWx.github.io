</script> <script> // Atlantic Hurricane Alerts
  async function fetchHurricaneAlerts() {
    const apiURL = "https://api.weather.gov/alerts/active";
    const hurricaneEvents = [
      "Hurricane Warning",
      "Hurricane Watch",
      "Tropical Storm Warning",
      "Tropical Storm Watch",
      "Storm Surge Warning",
      "Storm Surge Watch"
    ];

    try {
      const response = await fetch(apiURL);
      const data = await response.json();
      const alerts = data.features.filter(feature =>
        hurricaneEvents.includes(feature.properties.event)
      );

      const alertDiv = document.getElementById("hurricaneAlerts");
      if (alerts.length === 0) {
        alertDiv.innerHTML = "<strong>No active Atlantic hurricane alerts at this time.</strong>";
        return;
      }

      alerts.forEach((alert, index) => {
        const { event, headline, description, instruction, areaDesc, severity, effective, expires } = alert.properties;

        const wrapper = document.createElement("div");
        wrapper.className = "alertBox";
        wrapper.style.cssText = `
          border: 2px solid red;
          background-color: #fff3f3;
          padding: 1rem;
          margin-bottom: 1rem;
          border-radius: 8px;
          box-shadow: 0 2px 6px rgba(0,0,0,0.1);
          word-wrap: break-word;
          position: relative;
        `;

        const dismissBtn = document.createElement("button");
        dismissBtn.textContent = "✖";
        dismissBtn.title = "Dismiss alert";
        dismissBtn.style.cssText = `
          position: absolute;
          top: 8px;
          right: 10px;
          background: none;
          border: none;
          font-size: 1.2rem;
          cursor: pointer;
          color: #900;
        `;
        dismissBtn.onclick = () => wrapper.remove();

        const toggleBtn = document.createElement("button");
        toggleBtn.textContent = alerts.length > 1 ? "Show Details" : "Hide Details";
        toggleBtn.style.cssText = `
          margin-top: 0.5rem;
          background-color: #eee;
          border: 1px solid #ccc;
          border-radius: 4px;
          padding: 0.3rem 0.6rem;
          cursor: pointer;
        `;

        const detailDiv = document.createElement("div");
        detailDiv.style.display = alerts.length > 1 ? "none" : "block";
        detailDiv.innerHTML = `
          <strong>Area:</strong> ${areaDesc}<br>
          <strong>Effective:</strong> ${new Date(effective).toLocaleString()}<br>
          <strong>Expires:</strong> ${new Date(expires).toLocaleString()}<br><br>
          <strong>${headline}</strong><br>
          <p>${description.replace(/\n/g, "<br>")}</p>
          ${instruction ? `<p><em>${instruction.replace(/\n/g, "<br>")}</em></p>` : ""}
        `;

        toggleBtn.onclick = () => {
          const isHidden = detailDiv.style.display === "none";
          detailDiv.style.display = isHidden ? "block" : "none";
          toggleBtn.textContent = isHidden ? "Hide Details" : "Show Details";
        };

        wrapper.innerHTML = `<h3 style="margin-top: 0;">${event} – ${severity}</h3>`;
        wrapper.appendChild(dismissBtn);
        wrapper.appendChild(toggleBtn);
        wrapper.appendChild(detailDiv);
        alertDiv.appendChild(wrapper);
      });
    } catch (error) {
      console.error("Error fetching alerts:", error);
      document.getElementById("hurricaneAlerts").textContent = "Unable to fetch alerts.";
    }
  }

  fetchHurricaneAlerts();
</script> <script>
  async function fetchNHCKeyMessages() {
    const container = document.getElementById("nhcKeyMessages");
    container.innerHTML = "<strong>Loading Atlantic tropical system key messages...</strong>";

    try {
      const response = await fetch("https://www.nhc.noaa.gov/json/index.php?basin=at");
      const data = await response.json();

      if (data && data.activeStorms && data.activeStorms.length > 0) {
        container.innerHTML = "";

        const dismissedStorms = JSON.parse(localStorage.getItem("dismissedKeyMessages") || "[]");

        data.activeStorms.forEach(storm => {
          const stormId = storm.id || storm.name;

          if (dismissedStorms.includes(stormId)) return; // Skip if dismissed

          if (storm.keyMessages && storm.keyMessages.length > 0) {
            const wrapper = document.createElement("div");
            wrapper.style.border = "1px solid #ccc";
            wrapper.style.borderRadius = "8px";
            wrapper.style.padding = "1em";
            wrapper.style.marginBottom = "1em";
            wrapper.style.background = "#f9f9f9";
            wrapper.style.position = "relative";

            const closeBtn = document.createElement("button");
            closeBtn.textContent = "✖";
            closeBtn.style.position = "absolute";
            closeBtn.style.top = "10px";
            closeBtn.style.right = "10px";
            closeBtn.style.background = "transparent";
            closeBtn.style.border = "none";
            closeBtn.style.fontSize = "16px";
            closeBtn.style.cursor = "pointer";
            closeBtn.setAttribute("aria-label", "Dismiss");

            closeBtn.onclick = () => {
              wrapper.remove();
              const updated = [...dismissedStorms, stormId];
              localStorage.setItem("dismissedKeyMessages", JSON.stringify(updated));
            };

            const title = document.createElement("h3");
            title.textContent = `Key Messages for ${storm.name}`;
            title.style.color = "#b22222";
            title.style.marginTop = "0";

            wrapper.appendChild(closeBtn);
            wrapper.appendChild(title);

            storm.keyMessages.forEach(msg => {
              const p = document.createElement("p");
              p.textContent = `• ${msg}`;
              wrapper.appendChild(p);
            });

            container.appendChild(wrapper);
          }
        });

        if (container.innerHTML.trim() === "") {
          container.innerHTML = "All tropical system messages have been dismissed.";
        }
      } else {
        container.innerHTML = "No active tropical systems with key messages in the Atlantic basin.";
      }
    } catch (error) {
      console.error("Error fetching NHC key messages:", error);
      container.innerHTML = "Could not retrieve tropical system data at this time.";
    }
  }

  fetchNHCKeyMessages();
</script></div> </main>
