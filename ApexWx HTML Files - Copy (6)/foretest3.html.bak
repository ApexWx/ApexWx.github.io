<!DOCTYPE html>
<html>
  <head>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <title>foretest3</title>
  </head>
  <body>
    <p>
      <script>
  async function fetchForecastStructure() {
    try {
      const pointResponse = await fetch('https://api.weather.gov/points/47.25,-68.58');
      const pointData = await pointResponse.json();
      const forecastUrl = pointData?.properties?.forecast;
      if (!forecastUrl) throw new Error("Forecast URL not found");

      const forecastResponse = await fetch(forecastUrl);
      const forecastData = await forecastResponse.json();

      console.log("Forecast Data:", forecastData);
    } catch (err) {
      console.error("Error:", err);
    }
  }

  fetchForecastStructure();
</script><br> </p>
  </body>
</html>
